    "version": "0.2.0",
    "configurations":[
        {
            "name": "Debug dolphin CLI",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "psfgenerator",
                "-p", "../configs/default_psf_config_gibsonlanni.json",
                "-d", "${workspaceFolder}/results"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb"
        },
        {
            "name": "Debug cuda backend test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/backends/cuda/build/tests/cuda_test",
            "args": [],
            "stopAtEntry": false,
            "environment": [],
            "cwd": "${workspaceFolder}/backends/cuda/build",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                "description": "Enable pretty-printing",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
                }
            ]
        },

        {
            "name": "Debug dolphin config",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "deconvolution",
                "-c", "../configs/default_config.json"
            ],
            "additionalSOLibSearchPath": "${workspaceFolder}/lib/cube/**",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
        {
            "name": "Debug dolphin",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/mainTest/main_test",
            "args": [
                "${workspaceFolder}/configs/default_config.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [
            {
                "name": "LD_LIBRARY_PATH",
                "value": "${workspaceFolder}/backends/cuda/build"
            }],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
        {
          "name": "(ctest) Launch",
            "type": "cppdbg",
            "request": "launch",
            "additionalSOLibSearchPath": "${workspaceFolder}/lib/cube/**",
            "stopAtEntry": false,
            "environment": [],
            "cwd": "${cmake.testWorkingDirectory}",
            "program": "${cmake.testProgram}",
            "args": [ "${cmake.testArgs}"],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
       {
            "name": "Debug dolphin config psfgenerator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "psfgenerator",
                "-p", "../configs/default_psf_config_gibsonlanni.json",
                "-d", "${workspaceFolder}/results"
            ],
            "additionalSOLibSearchPath": "${workspaceFolder}/lib/cube/**",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
        {
            "name": "Debug dolphin GUI",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/gui/build/dolphin_gui",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb"
        },
        {
            "name": "Debug LabeledImageTest (CTest)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/labeled_image_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/tests",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                "description": "Enable pretty-printing",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
                }
            ]
        }
    ]
}