    "version": "0.2.0",
    "configurations":[
        {
            "name": "Debug dolphin CLI",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "psfgenerator",
                "-p", "../configs/default_psf_config_gibsonlanni.json",
                "-d", "${workspaceFolder}/results"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb"
        },
        {
            "name": "Debug dolphin config",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "deconvolution",
                "-c", "../configs/default_config.json"
            ],
            "additionalSOLibSearchPath": "${workspaceFolder}/lib/cube/**",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
        {
            "name": "Debug dolphin labeled image",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/frontends/cli/build/dolphin_cli",
            "args": [
                "deconvolution",
                "-c", "../tests/labeledImage/labeled_image_test.json"
            ],
            "additionalSOLibSearchPath": "${workspaceFolder}/lib/cube/**",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/frontends",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Disassemble in intel syntax",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
            ]
        },
